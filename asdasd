int main(void)
  
  {
      unsigned char this = 0xF0;
      unsigned char that = 0x55;

     
     binary_print(this);
     
     binary_print(that);
 
     binary_print(this & that);
     binary_print(this | that);
     binary_print(this ^ that);
     binary_print(~this);
     binary_print(this >> 2);
     binary_print(that << 1);
     
     }